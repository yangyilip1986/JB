<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CircleProgressBar">
        <attr name="circleProgressColor" format="color" />
        <attr name="circleProgressStrokeWidth" format="integer" />
    </declare-styleable>
    <declare-styleable name="TasksCompletedView">
        <attr name="radius" format="dimension" />
        <attr name="strokeWidth" format="dimension" />
        <attr name="circleColor" format="color" />
        <attr name="ringColor" format="color" />
        <attr name="word" format="string" />
    </declare-styleable>
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="border_overlay" format="boolean" />
    </declare-styleable>
        <declare-styleable name="ToggleButton">
        <attr name="borderWidth" format="dimension" />
        <attr name="offBorderColor" format="reference|color" />
        <attr name="offColor" format="reference|color" />
        <attr name="onColor" format="reference|color" />
        <attr name="spotColor" format="reference|color" />
        <attr name="animate" format="reference|boolean" />
        <attr name="isDefaultOn" format="reference|boolean" />
    </declare-styleable>

</resources>